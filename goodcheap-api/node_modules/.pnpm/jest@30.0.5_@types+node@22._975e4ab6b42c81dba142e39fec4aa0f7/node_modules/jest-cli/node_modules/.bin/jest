#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/goodcheap/goodcheap-api/node_modules/.pnpm/jest-cli@30.0.5_@types+node_14e2fc7cc80b5b3930bb524017786b9f/node_modules/jest-cli/bin/node_modules:/mnt/d/goodcheap/goodcheap-api/node_modules/.pnpm/jest-cli@30.0.5_@types+node_14e2fc7cc80b5b3930bb524017786b9f/node_modules/jest-cli/node_modules:/mnt/d/goodcheap/goodcheap-api/node_modules/.pnpm/jest-cli@30.0.5_@types+node_14e2fc7cc80b5b3930bb524017786b9f/node_modules:/mnt/d/goodcheap/goodcheap-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/goodcheap/goodcheap-api/node_modules/.pnpm/jest-cli@30.0.5_@types+node_14e2fc7cc80b5b3930bb524017786b9f/node_modules/jest-cli/bin/node_modules:/mnt/d/goodcheap/goodcheap-api/node_modules/.pnpm/jest-cli@30.0.5_@types+node_14e2fc7cc80b5b3930bb524017786b9f/node_modules/jest-cli/node_modules:/mnt/d/goodcheap/goodcheap-api/node_modules/.pnpm/jest-cli@30.0.5_@types+node_14e2fc7cc80b5b3930bb524017786b9f/node_modules:/mnt/d/goodcheap/goodcheap-api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jest.js" "$@"
else
  exec node  "$basedir/../../bin/jest.js" "$@"
fi
