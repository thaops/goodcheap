#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/goodcheap/goodcheap-api/node_modules/.pnpm/jest@30.0.5_@types+node@22._975e4ab6b42c81dba142e39fec4aa0f7/node_modules/jest/bin/node_modules:/mnt/d/goodcheap/goodcheap-api/node_modules/.pnpm/jest@30.0.5_@types+node@22._975e4ab6b42c81dba142e39fec4aa0f7/node_modules/jest/node_modules:/mnt/d/goodcheap/goodcheap-api/node_modules/.pnpm/jest@30.0.5_@types+node@22._975e4ab6b42c81dba142e39fec4aa0f7/node_modules:/mnt/d/goodcheap/goodcheap-api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/goodcheap/goodcheap-api/node_modules/.pnpm/jest@30.0.5_@types+node@22._975e4ab6b42c81dba142e39fec4aa0f7/node_modules/jest/bin/node_modules:/mnt/d/goodcheap/goodcheap-api/node_modules/.pnpm/jest@30.0.5_@types+node@22._975e4ab6b42c81dba142e39fec4aa0f7/node_modules/jest/node_modules:/mnt/d/goodcheap/goodcheap-api/node_modules/.pnpm/jest@30.0.5_@types+node@22._975e4ab6b42c81dba142e39fec4aa0f7/node_modules:/mnt/d/goodcheap/goodcheap-api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/jest@30.0.5_@types+node@22._975e4ab6b42c81dba142e39fec4aa0f7/node_modules/jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../.pnpm/jest@30.0.5_@types+node@22._975e4ab6b42c81dba142e39fec4aa0f7/node_modules/jest/bin/jest.js" "$@"
fi
